<div id="wrapper">
  <!-- Top Bar Start -->
  <!-- ============================================================== -->
  @await Html.PartialAsync("_LayoutHeader")
  <!-- Start right Content here -->
  <!-- ============================================================== -->
  <div class="content-page">
    <!-- Start content -->
    <div class="content">

      <div class="container-fluid">
        <div class="page-title-box">
          <div class="row align-items-center">
            <div class="col-sm-6">
              <h4 class="page-title">Assgin Mentor</h4>
              <ol class="breadcrumb">
                <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Home</a></li>
                <li class="breadcrumb-item"><a href="javascript:void(0);">Assign Mentors</a></li>
              </ol>
            </div>
            <div class="col-sm-6">
              <div class="float-right d-none d-md-block">
                <div class="dropdown">
                  <button class="btn btn-primary dropdown-toggle arrow-none waves-effect waves-light" data-toggle="modal" data-target=".bs-example-modal-lg" aria-expanded="false">
                    + Assign Mentor
                  </button>
                  <div class="dropdown-menu dropdown-menu-right">
                    <a class="dropdown-item"
                       href="#">Action</a> <a class="dropdown-item" href="#">Another action</a>
                    <a class="dropdown-item" href="#">Something else here</a>
                    <div class="dropdown-divider"></div><a class="dropdown-item"
                                                           href="#">Separated link</a>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- end row -->
        <!-- Begining of the modal-->
        <!--  Modal content for the above example -->
        <div class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog"
             aria-labelledby="myLargeModalLabel" aria-hidden="true">
          <div class="modal-dialog modal-lg">
            <div class="modal-content">
              <div class="modal-header">
                <h5 class="modal-title mt-0" id="myLargeModalLabel">
                  Assign Mentor
                </h5><button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                  ×
                </button>
              </div>
              <div class="modal-body">
                <div class="container-fluid">
                  <div class="row">
                    <div class="col-md-12">
                      <div class="row ">
                        <div class="form-group col-md-5">
                          <label>Team</label>
                          <input type="text" hidden id="autoTeamId" />
                          <input type="text" id="autoTeam" name="autoTeam" class="form-control" placeholder="Team Positive">
                        </div>
                        <div class="form-group col-md-1 align-content-center">
                          <i class="mdi mdi-account-switch mdi-48px m-b-15"></i>
                        </div>
                        <div class="form-group col-md-5">
                          <label>Mentor</label>
                          <input type="text" hidden id="autoMentorId" />
                          <input type="text" autocomplete="on" class="colorpicker-default form-control" placeholder="Prof Jang">
                        </div>

                      </div>

                    </div>
                  </div>
                </div>
              </div>
              <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary">Submit</button>
              </div>
            </div><!-- /.modal-content -->
          </div><!-- /.modal-dialog -->
        </div><!-- /.modal -->
        <!-- End of the Modal-->
        <!-- end row -->

        <div class="row">
          <div class="col-12">
            <div class="card">
              <div class="card-body">
                <h4 class="mt-0 header-title"></h4>
                <div class="row">
                  <div class="col-12">
                    <div class="card">
                      <div class="card-body">
                        <h4 class="mt-0 header-title m-b-20"></h4>
                        <form class="repeater" enctype="multipart/form-data">
                          <div data-repeater-list="group-a">
                            <div data-repeater-item class="row">
                              <div class="form-group col-lg-3"></div>
                              <div class="form-group col-lg-6">
                                <label for=""></label> <input type="text" id="name" placeholder=" To search, type name, company name or email and press Enter ......" class="form-control">
                              </div>

                              @*<div class="col-lg-1 align-self-center">
                                    <input data-repeater-delete type="button" class="btn btn-primary btn-block" value="Search">
                                </div>*@
                            </div>
                          </div>
                        </form>
                      </div>
                    </div>
                  </div>
                </div><!-- end row -->
                @*<p class="text-muted m-b-30">
                      DataTables has most features enabled by default, so all
                      you need to do to use it with your own tables is to call the construction
                      function: <code>$().DataTable();</code>.
                  </p>*@
                <table id="datatable-buttons" class="table table-bordered dt-responsive nowrap"
                       style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                  <thead>
                    <tr>
                      <th>Venture Name</th>
                      <th>Mentor</th>
                      <th>Date</th>
                      <th>Action</th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr>
                      <td>Tiger Nixon</td>
                      <td>System Architect</td>
                      <td>2011/04/25</td>
                      <td><a asp-action="TeamInfo" class="btn btn-primary">View Info</a></td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>
          </div><!-- end col -->
        </div><!-- end row -->
      </div><!-- container-fluid -->
    </div>
  </div>
</div>

