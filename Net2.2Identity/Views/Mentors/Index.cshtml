<script type="text/javascript">
  $(document).ready(function () {
    $('#DataTable').DataTable();
    $('#checkBoxAll').click(function () {
      if ($(this).is(":checked")) {
        $(".chkCheckBoxId").prop("checked", true)
      }
      else {
        $(".chkCheckBoxId").prop("checked", false)
      }
    });
  });
</script>

<script type="text/javascript">


          $(document).delegate('#viewProfile', 'click', function (e) {

            firstname = $(this).attr('sFirst');
            profileurl = $(this).attr('sProfileURl');
            imageurl = $(this).attr('sImageUrl');




          //console.log(description, department);

            $('input[name=ssFirstName]').val(firstname);
            //$('input[name=ssProfileUrl]').href(profileurl);
            //$('input[name=imageUrl]').src(imageurl);
            document["imageUrl"].src = "/" + imageurl;
            var a = document.getElementById('ssProfileUrl');
            a.href = "/" + profileurl;


            $('input[name=ssLastName]').val($(this).attr('sLastName'));
            $('input[name=ssOtherName]').val($(this).attr('sOtherName'));
            $('input[name=ssNickName]').val($(this).attr('sNickName'));
            $('input[name=ssEmail]').val($(this).attr('sEmail'));
            $('input[name=ssPhoneNumberMobile]').val($(this).attr('sPhoneNumberMobile'));
            $('input[name=ssPhoneNumberWork]').val($(this).attr('sPhoneNumberWork'));
            $('input[name=ssAddress]').val($(this).attr('sAddress'));
            $('input[name=ssCity]').val($(this).attr('sCity'));
            $('input[name=ssProvince]').val($(this).attr('sProvince'));
            $('input[name=ssCountry]').val($(this).attr('sCountry'));
            $('input[name=ssIndustry]').val($(this).attr('sIndustry'));
            $('input[name=ssExpertise]').val($(this).attr('sExpertise'));
            $('input[name=ssInterest]').val($(this).attr('sInterest'));
            $('input[name=ssProfile]').val($(this).attr('sProfile'));


            //$('input[name=eEmailAddress]').val(email);
            //$('input[name=eTotalAmount]').val(amount);

        });


  $(document).ready(function () {

    $("#sendMailList").click(function (event) {

      event.preventDefault();

      var yourArray = new Array();
      var emaillist = "";

      $("#datatable-buttons tbody tr input:checkbox").each(function () {

        if ($(this).is(':checked')) {
          yourArray.push(this.name);

          emaillist = emaillist + this.name + ',';
        }

        //yourArray.push($(this).val());
      });


      console.log(emaillist)
      window.open("mailto:" + emaillist, "_self")
    });


    $("#setActive").click(function (event) {

      event.preventDefault();

      var yourArray = new Array();
      var mentids = "";

      $("#datatable-buttons tbody tr input:checkbox").each(function () {

        if ($(this).is(':checked')) {
          yourArray.push(this.name);

          mentids = mentids + this.value + ',';
        }

        //yourArray.push($(this).val());
      });

      $.ajax({
        type: "POST",
        url: "/Mentors/SetActive",
        data: JSON.stringify(mentids),
        dataType: "json",
        contentType: 'application/json; charset=utf-8',
        headers: {
          RequestVerificationToken:
            $('input:hidden[name="__RequestVerificationToken"]').val()
        },
        success: function (r) {

          if (r.msg == "Success") {

            window.location.reload();

          } else if (r.msg == "Fail") {

            alert(r.msg);

          } else if (r.msg == "No Data") {
            alert(r.msg);

          } else {
            alert(r.msg);
          }
          //alert(r.msg);
          // window.location.reload();

        },
        error: function () {
          alert(r.msg);
        }
      });

    });

    $("#setInactive").click(function (event) {

      event.preventDefault();

      var yourArray = new Array();
      var mentids = "";

      $("#datatable-buttons tbody tr input:checkbox").each(function () {

        if ($(this).is(':checked')) {
          yourArray.push(this.name);

          mentids = mentids + this.value + ',';
        }

        //yourArray.push($(this).val());
      });

      $.ajax({
        type: "POST",
        url: "/Mentors/SetInActive",
        data: JSON.stringify(mentids),
        dataType: "json",
        contentType: 'application/json; charset=utf-8',
        headers: {
          RequestVerificationToken:
            $('input:hidden[name="__RequestVerificationToken"]').val()
        },
        success: function (r) {

          if (r.msg == "Success") {

            window.location.reload();

          } else if (r.msg == "Fail") {

            alert(r.msg);

          } else if (r.msg == "No Data") {
            alert(r.msg);

          } else {
            alert(r.msg);
          }
          //alert(r.msg);
          // window.location.reload();

        },
        error: function () {
          alert(r.msg);
        }
      });

    });

    $("#delete").click(function (event) {

      event.preventDefault();

      var yourArray = new Array();
      var mentids = "";

      $("#datatable-buttons tbody tr input:checkbox").each(function () {

        if ($(this).is(':checked')) {
          yourArray.push(this.name);

          mentids = mentids + this.value + ',';
        }

        //yourArray.push($(this).val());
      });

      var result = confirm("Want to delete mentor(s)?");
      if (result) {

        $.ajax({
          type: "POST",
          url: "/Mentors/Delete",
          data: JSON.stringify(mentids),
          dataType: "json",
          contentType: 'application/json; charset=utf-8',
          headers: {
            RequestVerificationToken:
              $('input:hidden[name="__RequestVerificationToken"]').val()
          },
          success: function (r) {

            if (r.msg == "Success") {

              window.location.reload();

            } else if (r.msg == "Fail") {

              alert(r.msg);

            } else if (r.msg == "No Data") {
