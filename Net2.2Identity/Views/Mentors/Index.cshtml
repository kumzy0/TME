                    <tr>
                      <td><input type="checkbox" name="@item.Email" value="@item.Id" class="custom-checkbox chkCheckBoxId" /></td>
                      <td>@item.FirstName</td>
                      <td>@item.LastName</td>
                      <td>@item.PhoneNumberMobile</td>
                      <td>@item.Email</td>
                      <td>
                        @if (item.IsActive)
                          {
                        <a class="btn btn-sm btn-success">Active</a>
                          }
                          else
                          {
                        <a class="btn btn-sm btn-danger">Not Active</a>
                          }
                      </td>
                      <td>@item.DateCreated.ToString("MMM dd, yyyy")</td>
                      <td><btn name="viewProfile" id="viewProfile" class="btn btn-primary"  data-toggle="modal" data-target=".bs-example-modal-lg2" 
                               
