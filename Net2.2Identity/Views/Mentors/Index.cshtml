@model IEnumerable<TME.Models.Mentor>
@{
  ViewData["Title"] = "Mentor";
}

<style>
  .modal-lg {
    max-width: 60% !important;
  }
</style>




<div id="wrapper">
  <!-- Top Bar Start -->
  <!-- ============================================================== -->
  @await Html.PartialAsync("_LayoutHeader")
  <!-- Start right Content here -->
  <!-- ============================================================== -->
  <div class="content-page">
    <!-- Start content -->
    <div class="content">

      <div class="container-fluid">
        <div class="page-title-box">
          <div class="row align-items-center">
            <div class="col-sm-6">
              <h4 class="page-title">Mentors</h4>
              <ol class="breadcrumb">
                <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Home</a></li>
                <li class="breadcrumb-item"><a href="javascript:void(0);">Mentors</a></li>
                @*<li class="breadcrumb-item active">Data Table</li>*@
              </ol>
            </div>
            <div class="col-sm-6">
              <div class="float-right d-none d-md-block">
                <div class="dropdown">
                  <button class="btn btn-primary dropdown-toggle arrow-none waves-effect waves-light" data-toggle="modal" data-target=".bs-example-modal-lg" aria-expanded="false">
                    + Add Mentor
                  </button>
                  <div class="dropdown-menu dropdown-menu-right">
                    <a class="dropdown-item"
                       href="#">Action</a> <a class="dropdown-item" href="#">Another action</a>
                    <a class="dropdown-item" href="#">Something else here</a>
                    <div class="dropdown-divider"></div><a class="dropdown-item"
                                                           href="#">Separated link</a>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- end row -->
        <!-- Begining of the modal-->
        <!--  Modal content for the above example -->
        <div class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog"
             aria-labelledby="myLargeModalLabel" aria-hidden="true">
          <div class="modal-dialog modal-lg">
            <div class="modal-content">
              <div class="modal-header">
                <h5 class="modal-title mt-0" id="myLargeModalLabel">
                  Add Mentor
                </h5><button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                  ×
                </button>
              </div>
              <div class="modal-body">
                <form enctype="multipart/form-data" asp-action="AddMentor" method="post">

                  <div class="container-fluid">
                    <div class="row">
                      <div class="col-md-12">

                        <div class="row">
                          <div class="col-md-9">
                            <div class="row ">
                              <div class="form-group col-md-4">
                                <label>First Name*</label>
                                <input type="text" required id="FirstName" name="FirstName" class="colorpicker-default form-control" placeholder="Firstname">
                              </div>
                              <div class="form-group col-md-4">
                                <label>Last Name*</label>
                                <input type="text" required id="LastName" name="LastName" class="colorpicker-default form-control" placeholder="Lastname">
                              </div>
                              <div class="form-group col-md-4">
                                <label>Nick Name</label>
                                <input type="text" id="NickName" name="NickName" class="colorpicker-default form-control" placeholder="Nick Name">
                              </div>
                            </div>

                            <div class="row ">
                              <div class="form-group col-md-4">
                                <label>Email*</label>
                                <input type="email" required id="Email" name="Email" class="colorpicker-default form-control" placeholder="email@website.com">
                              </div>
                              <div class="form-group col-md-4">
                                <label>Phone Number 1*</label>
                                <input type="text" required id="PhoneNumberMobile" name="PhoneNumberMobile" class="colorpicker-default form-control" placeholder="Primary (mobile)">
                              </div>
                              <div class="form-group col-md-4">
                                <label>Phone Number 2</label>
                                <input type="text" id="PhoneNumberWork" name="PhoneNumberWork" class="colorpicker-default form-control" placeholder="Secondary (work)">
                              </div>
                            </div>

                            <div class="row ">
                              <div class="form-group col-md-12">
                                <label> Address*</label>
                                <input type="text" required id="Address" name="Address" class="colorpicker-default form-control" placeholder="Street No & Name" />
                              </div>
                            </div>

                          </div>
                          <div class="col-md-3 ">
                            <label> Headshot</label>
                            <div class="fallback dropzone"><input name="file" type="file" multiple="multiple"></div>

                          </div>
                        </div>


                        <div class="row ">
                          <div class="form-group col-md-4">
                            <label>City*</label>
                            <input type="text" required id="City" name="City" class="colorpicker-default form-control" placeholder="City">
                          </div>
                          <div class="form-group col-md-4">
                            <label>Province*</label>
                            <input type="text" required id="Province" name="Province" class="colorpicker-default form-control" placeholder="Province">
                          </div>
                          <div class="form-group col-md-4">
                            <label>Country*</label>
                            <input type="text" required id="Country" name="Country" class="colorpicker-default form-control" placeholder=" Country">
                          </div>
                        </div>

                        <div class="row ">
                          <div class="form-group col-md-4">
                            <label>Industires</label>
                            <textarea type="text" id="Industry" name="Industry" class="colorpicker-default form-control" placeholder="List of Indusries"></textarea>
                          </div>
                          <div class="form-group col-md-4">
                            <label>Expertise</label>
                            <textarea type="text" id="Expertise" name="Expertise" class="colorpicker-default form-control" placeholder="List of Areas of Exprtise"></textarea>
                          </div>
                          <div class="form-group col-md-4">
                            <label>Interest</label>
                            <textarea type="text" id="Interest" name="Interest" class="colorpicker-default form-control" placeholder="List of Interests"></textarea>
                          </div>
                        </div>

                        <div class="row ">
                          <div class="form-group col-md-8">
                            <label>Profile</label>
                            <textarea type="text" id="Profile" name="Profile" class="colorpicker-default form-control" placeholder="Brief Profile"></textarea>
                          </div>
                          <div class="form-group col-md-4">
                            <label>Upload Profile</label>
                            <input type="file" class="form-control-file" name="resume" id="resume">
                          </div>
                        </div>

                      </div>



                    </div>
                  </div>

                  <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="submit" class="btn btn-primary" >Add Mentor</button>
                  </div>
                </form>
                @*id="submitMentor" name="submitMentor"*@
              </div>

            </div><!-- /.modal-content -->
          </div><!-- /.modal-dialog -->
        </div><!-- /.modal -->
        <!-- End of the Modal-->
        <!-- end row -->

        <div class="row">
          <div class="col-12">
            <div class="card">
              <div class="card-body">
                <h4 class="mt-0 header-title"></h4>
                <div class="row">
