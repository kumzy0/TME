                                  <input type="text" readonly id="ssProvince" name="ssProvince" class="colorpicker-default form-control" placeholder="Province">
                                </div>
                                <div class="form-group col-md-4">
                                  <label>Country*</label>
                                  <input type="text" readonly id="Country" name="Country" class="colorpicker-default form-control" placeholder=" Country">
                                </div>
                              </div>

                              <div class="row ">
                                <div class="form-group col-md-4">
                                  <label>Industires</label>
                                  <textarea type="text" readonly id="ssIndustry" name="ssIndustry" class="colorpicker-default form-control" placeholder="List of Indusries"></textarea>
                                </div>
                                <div class="form-group col-md-4">
                                  <label>Expertise</label>
                                  <textarea type="text" readonly id="ssExpertise" name="ssExpertise" class="colorpicker-default form-control" placeholder="List of Areas of Exprtise"></textarea>
                                </div>
                                <div class="form-group col-md-4">
                                  <label>Interest</label>
                                  <textarea type="text" readonly id="ssInterest" name="ssInterest" class="colorpicker-default form-control" placeholder="List of Interests"></textarea>
                                </div>
                              </div>

                              <div class="row ">
                                <div class="form-group col-md-8">
                                  <label>Profile</label>
                                  <textarea type="text" readonly id="ssProfile" name="ssProfile" class="colorpicker-default form-control" placeholder="Brief Profile"></textarea>
                                </div>
                                <div class="form-group col-md-4">
                                  <label>Uploaded Profile</label>
                                  <br />
                                  <a id="ssProfileUrl" target="_blank" name="ssProfileUrl" href="">Download Profile</a>

                                </div>
                              </div>

                            </div>



                          </div>
                        </div>

                        <div class="modal-footer">
                          <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                          @*<button type="submit" class="btn btn-primary">Add Mentor</button>*@
                        </div>
                      </form>
                      @*id="submitMentor" name="submitMentor"*@
                    </div>

                  </div><!-- /.modal-content -->
                </div><!-- /.modal-dialog -->
              </div><!-- /.modal -->

            </div>
          </div><!-- end col -->
        </div><!-- end row -->
      </div><!-- container-fluid -->
