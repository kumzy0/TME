                      <th>Last Name</th>
                      <th>Cell</th>
                      <th>Email</th>
                      <th>Status</th>
                      <th>Date Added</th>
                      <th>View Details</th>

                    </tr>
                  </thead>
                  <tbody>
                    @foreach (var item in Model)
