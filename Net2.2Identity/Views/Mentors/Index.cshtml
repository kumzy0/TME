                               sProfileURl="@item.ProfileUrl"
                               sImageUrl="@item.ImageUrl"
                               sLastName="@item.LastName"
                               sOtherName="@item.OtherName"
                               sNickName="@item.NickName"
                               sEmail="@item.Email"
                               sPhoneNumberMobile="@item.PhoneNumberMobile"
                               sPhoneNumberWork="@item.PhoneNumberWork"
                               sAddress="@item.Address"
                               sCity="@item.City"
                               sProvince="@item.Province"
                               sCountry="@item.Country"
                               sIndustry="@item.Industry"
                               sExpertise="@item.Expertise"
                               sInterest="@item.Interest"
                               sProfile="@item.Profile"

                               >View Info</btn></td>

                    </tr>
                    }
